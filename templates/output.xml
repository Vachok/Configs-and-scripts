<templateSet group="output">
  <template name="soutm" value="LOGGER.info(&quot;$CLASS_NAME$.$METHOD_NAME$&quot;);" description="Prints current class and method names to System.out" toReformat="true" toShortenFQNames="true">
    <variable name="CLASS_NAME" expression="className()" defaultValue="" alwaysStopAt="false" />
    <variable name="METHOD_NAME" expression="methodName()" defaultValue="" alwaysStopAt="false" />
    <context />
  </template>
  <template name="sout" value="LOGGER.info($END$);" description="Prints a string to System.out" toReformat="true" toShortenFQNames="true">
    <context />
  </template>
</templateSet>