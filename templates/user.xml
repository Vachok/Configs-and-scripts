<templateSet group="user">
  <template name="ttt" value="$DATE$ $TIME$" shortcut="SPACE" description="" toReformat="false" toShortenFQNames="true">
    <variable name="DATE" expression="date()" defaultValue="" alwaysStopAt="false" />
    <variable name="TIME" expression="time()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="cw" value="Crashlytics.log(Log.WARN, TAG, &quot;$VAR$&quot;);" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="methodName()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="er" value="Crashlytics.log(Log.ERROR, TAG, AbstractForms.fromArray(e));" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="ci" value="Crashlytics.log(Log.INFO, TAG, $END$);" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>