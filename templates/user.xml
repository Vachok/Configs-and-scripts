<templateSet group="user">
  <template name="ttt" value="$DATE$ $TIME$" shortcut="SPACE" description="" toReformat="false" toShortenFQNames="true">
    <variable name="DATE" expression="date()" defaultValue="" alwaysStopAt="false" />
    <variable name="TIME" expression="time()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="cw" value="Crashlytics.log(Log.WARN, TAG, &quot;$VAR$&quot;);" description="" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="methodName()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="er" value="Crashlytics.log(Log.ERROR, TAG, AbstractForms.fromArray(e));" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="ci" value="Crashlytics.log(Log.INFO, TAG, $END$);" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="thi" value="throw new IllegalStateException(&quot;$CLASS$.$VAR$&quot;);" description="illegal state" toReformat="false" toShortenFQNames="true">
    <variable name="CLASS" expression="className()" defaultValue="" alwaysStopAt="false" />
    <variable name="VAR" expression="methodName()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="af" value="AbstractForms.fromArray($END$)" description="AbstractForms" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="err" value="Context applicationContext = FirebaseApp.getInstance().getApplicationContext();&#10;Intent intent = new Intent(applicationContext, ErrActivity.class);&#10;intent.putExtra(ConstantsFor.RESOURCES.getString(R.string.error_intent_param_name), AbstractForms.fromArray(e));&#10;intent.addFlags(Intent.FLAG_ACTIVITY_NEW_TASK);&#10;applicationContext.startActivity(intent);&#10;" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="tr" value="trace.putAttribute(&quot;$METHOD$&quot;, $PARAMS$);" description="" toReformat="false" toShortenFQNames="true">
    <variable name="METHOD" expression="methodName()" defaultValue="" alwaysStopAt="false" />
    <variable name="PARAMS" expression="showParameterInfo()" defaultValue="SystemClock.elapsedRealtime()+ConstantsFor.ELAPSED_REALTIME" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>