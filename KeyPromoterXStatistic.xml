<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Generate...">
        <Item>
          <IdeaActionID>Generate</IdeaActionID>
          <ShortCut>'Alt+Insert'</ShortCut>
          <Description>Generate...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project Directory">
        <Item>
          <IdeaActionID>FileChooser.GotoProject</IdeaActionID>
          <ShortCut>'Ctrl+2'</ShortCut>
          <Description>Project Directory</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Main'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Shift+F10'</ShortCut>
          <Description>Run 'Main'</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'Main'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop 'Main'</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Update Project">
        <Item>
          <IdeaActionID>Vcs.UpdateProject</IdeaActionID>
          <ShortCut>'Ctrl+T'</ShortCut>
          <Description>Update Project</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>