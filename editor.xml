<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="13" />
    <option name="FONT_FAMILY" value="Lucida Sans Typewriter" />
    <option name="USE_LIGATURES" value="true" />
    <option name="SECONDARY_FONT_FAMILY" value="Consolas" />
  </component>
  <component name="EditorSettings">
    <option name="CARET_BLINKING_PERIOD" value="800" />
    <option name="IS_RIGHT_MARGIN_SHOWN" value="false" />
    <option name="ARE_LINE_NUMBERS_SHOWN" value="false" />
    <option name="ARE_GUTTER_ICONS_SHOWN" value="false" />
    <option name="IS_FOLDING_OUTLINE_SHOWN" value="false" />
    <option name="SHOW_BREADCRUMBS" value="false" />
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
    <option name="IS_ANIMATED_SCROLLING" value="false" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern icon="important" useCustomColors="true" pattern="notcompil">
      <option name="FOREGROUND" value="90a41" />
      <option name="BACKGROUND" value="f3ff00" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="fffffb" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bMARK\b.*">
      <option name="FOREGROUND" value="fffffb" />
      <option name="BACKGROUND" value="3ff" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="effbff" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
  </component>
</application>